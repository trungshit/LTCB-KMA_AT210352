#include <stdio.h>
#include <stdlib.h>

void kt(int m,int n,int a[m][n])
{
    int i,gt=1;
    int tren=0,duoi=m-1,trai=0,phai=n-1;
    while(gt<=m*n)
    {
        for(i=trai;i<=phai&><=m*n;i++)
        {
            a[tren][i]=gt++;
        }
        tren++;
        for(i=tren;i<=duoi&><=m*n;i++)
        {
            a[i][phai]=gt++;
        }
        phai--;
        for(i=phai;i>=trai&><=m*n;i--)
        {
            a[duoi][i]=gt++;
        }
        duoi--;
        for(i=duoi;i>=tren&><=m*n;i--)
        {
            a[i][trai]=gt++;
        }
        trai++;
    }
}
void ra(int m,int n,int a[m][n])
{
    int i,j;
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            printf("%4d",a[i][j]);
        }
        printf("\n");
    }
}
int main()
{
    //freopen("D:\\C\\input.txt","r",stdin);
    int m,n;
    scanf("%d %d",&m,&n);
    int a[m][n];
    kt(m,n,a);
    ra(m,n,a);
    return 0;
}
